{
  "name": "electron-react-typescript",
  "version": "0.0.7",
  "description": "",
  "homepage": "https://github.com/Robinfr/electron-react-typescript",
  "bugs": {
    "url": "https://github.com/Robinfr/electron-react-typescript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Robinfr/electron-react-typescript.git"
  },
  "license": "SEE LICENSE",
  "author": {
    "name": "Your name",
    "email": "Your email"
  },
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "cross-env NODE_ENV=production webpack --config config/webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config config/webpack.renderer.prod.config.js",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "pack": "npm run build && electron-builder --dir",
    "re:build": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -clean-world -w",
    "prestart": "npm run build",
    "start": "electron .",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "start-main-dev": "webpack --config config/webpack.main.config.js && electron ./dist/main.js",
    "start-renderer-dev": "webpack-dev-server --config config/webpack.renderer.dev.config.js",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "store": "webpack-dev-server --watch --config store.webpack.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "eslint --ext=jsx,js,ts,tsx --fix src"
    ],
    "{src,test,mocks}/**/*.{json,css,scss,md}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@ambientlight/bs-rx": "^0.2.3",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@jsiebern/bs-material-ui": "^1.3.0",
    "@jsiebern/bs-material-ui-ppx": "^1.0.6",
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@mscharley/bs-material-ui-icons": "^1.0.2",
    "@types/async": "^3.2.3",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.0",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webpack": "^4.41.13",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-loader": "^8.0.6",
    "bs-fetch": "^0.5.2",
    "bs-platform": "^7.3.2",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.1",
    "electron": "9.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "fp-ts": "^2.6.6",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "immer": "^6.0.9",
    "jest": "^24.8.0",
    "node-sass": "^4.14.1",
    "observable-process": "^7.0.2",
    "prettier": "^1.18.2",
    "purify-ts": "^0.15.1",
    "ramda": "^0.27.0",
    "re-classnames": "^4.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "reason-promise": "^1.0.2",
    "reason-react": "^0.8.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.5",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.1",
    "typeface-roboto": "^0.0.75",
    "typesafe-actions": "^5.1.0",
    "typescript": "^3.4.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "build": {
    "productName": "ProductName",
    "appId": "org.your.productname",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {}
}
